{
  "name": "react-hono-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "packageManager": "bun@1.2.15",
  "engines": {
    "node": ">=22.14.0",
    "bun": ">=1.2.15"
  },
  "imports": {
    "#server/*": "./server/*",
    "#client/*": "./src/*"
  },
  "scripts": {
    "start": "NODE_ENV=production bun with-env bun dist/index.js",
    "build": "bun with-env vite build && bun build --minify --sourcemap --target=bun --outdir=dist server/index.ts",
    "build:docker": "docker build -t react-hono-template .",
    "preview": "bun with-env vite preview",
    "clean": "git clean -xdf .cache .turbo dist node_modules .tanstack",
    "dev:server": "bun with-env tsx watch server/index.ts",
    "dev:vite": "wait-on http://localhost:4001/health && bun with-env vite",
    "dev": "concurrently \"bun dev:server\" \"bun dev:vite\"",
    "format": "prettier --check . --ignore-path .gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e .env --",
    "test": "vitest run",
    "auth:generate": "bun dlx @better-auth/cli generate --output server/drizzle/schema/auth.ts --config server/lib/auth.ts",
    "db:push": "bun with-env drizzle-kit push",
    "db:studio": "bun with-env drizzle-kit studio",
    "ui:add": "bunx --bun shadcn add"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@eslint/js": "^9.33.0",
    "@hono/node-server": "^1.14.4",
    "@hookform/resolvers": "^5.2.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query-devtools": "^5.86.0",
    "@tanstack/react-router": "^1.121.34",
    "@tanstack/react-table": "^8.21.2",
    "better-auth": "^1.3.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "embla-carousel-react": "^8.6.0",
    "hono": "^4.9.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.542.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.6",
    "nuqs": "^2.5.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.5",
    "recharts": "2.15.4",
    "shadcn": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "typescript-eslint": "^8.40.0",
    "vaul": "^1.1.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.7",
    "@eslint/compat": "^1.3.2",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/router-devtools": "^1.99.9",
    "@tanstack/router-plugin": "^1.99.12",
    "@types/bun": "^1.2.21",
    "@types/node": "^22.15.29",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "3.2.4",
    "concurrently": "^9.2.0",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-turbo": "^2.5.6",
    "globals": "^15.14.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.8.3",
    "vite": "6.1.0",
    "wait-on": "^8.0.4"
  },
  "prettier": "./prettier.config.js"
}
